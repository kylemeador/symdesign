<!--					<FastDesign name="int_design" task_operations="init,vrt,evolution_cutoff1,fssm_cutoff1,evolution_cutoff2,fssm_cutoff2,hbnet_rtr,des_around_total" movemap_factory="design_map" scorefxn="2015_nano_cst" repeats="1" relaxscript="PolarDesign2019" ramp_down_constraints="false" disable_design="false" delete_virtual_residues_after_FastRelax="false"/> # cgs="coord_cst_gen" min_type="lbfgs_armijo_nonmonotone" bondangle="false" bondlength="false"-->
		<FastDesign name="int_design" task_operations="init,vrt,pssm_cutoff1,pssm_cutoff2,hbnet_rtr,des_around_total" movemap_factory="design_map" scorefxn="2015_cst" repeats="1" relaxscript="PolarDesign2019" ramp_down_constraints="false" disable_design="false" delete_virtual_residues_after_FastRelax="false"/> # cgs="coord_cst_gen" min_type="lbfgs_armijo_nonmonotone" bondangle="false" bondlength="false"
<!--		<FastDesign name="int_design" task_operations="init,vrt,pssm_cutoff1,pssm_cutoff2,hbnet_rtr,des_around_total" movemap_factory="design_map" scorefxn="2015_nano_cst" repeats="1" relaxscript="PolarDesign2019" ramp_down_constraints="false" disable_design="false" delete_virtual_residues_after_FastRelax="false"/> # cgs="coord_cst_gen" min_type="lbfgs_armijo_nonmonotone" bondangle="false" bondlength="false"-->
		# sample all rotamers to find minimum
